{
  "extends": "@antfu",
  "rules": {
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": {
          "max": 3
        },
        "multiline": {
          "max": 1
        }
      }
    ],
    "no-console": "warn",
    "@typescript-eslint/consistent-type-definitions": ["warn", "type"],
    "@typescript-eslint/brace-style": ["warn", "1tbs", { "allowSingleLine": false }],
    "curly": ["warn", "multi-line"],
    "react/prop-types": "off"
  }
}